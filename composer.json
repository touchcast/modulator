{
  "name": "vyg/modulator",
  "description": "A SilverStripe module for dynamically creating pages from smaller components",
  "type": "silverstripe-vendormodule",
  "require": {
    "silverstripe/vendor-plugin": "^1",
    "silverstripe/framework": "^4.1",
    "symbiote/silverstripe-gridfieldextensions": "^3.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~3.7"
  },
  "autoload": {
    "psr-4": {
      "Voyage\\Modulator": "src/"
    }
  },
  "extra": {
    "expose": [
      "css",
      "javascript"
    ]
  }
}
